@model Project
@{
    ViewData["Title"] = "GetById";
}

<h1 style="color:white;" >Project Information</h1>



<div style="color:black;" class="card text-center">
    <div class="card-header">
        <h1>Project Data</h1>
    </div>
    <div class="card-body">
        <h5 class="card-title">Name : @Model.Name</h5>
        <br />
        <h5 class="card-title">Description : @Model.Description</h5>
        <br />
        <a asp-controller ="Project" asp-action="index" class="btn btn-primary">Back to list</a>
    </div>
</div>

