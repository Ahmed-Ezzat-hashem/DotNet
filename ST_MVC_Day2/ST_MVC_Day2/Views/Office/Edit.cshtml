@model Office
@{
    ViewData["Title"] = "EditForm";
}

<h1 style="color:white;" >Edit Office Data</h1>

<form action="/Office/Edit" method="post" style="color:white;">
    <input type="number" id="ID" name="ID" value="@Model.ID" hidden />
    <div class="mb-3">
        <label asp-for="Name" class="form-label"></label>
        <input asp-for="Name" class="form-control" value="@Model.Name">
    </div>
    <div class="mb-3">
        <label asp-for="Location" class="form-label">Location</label>
        <input asp-for="Location" class="form-control" value="@Model.Location">
    </div>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>